<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="410.0" minWidth="660.0" prefHeight="650.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.egret.BoardEditor.MainMenu">
   <top>
      <Label text="Welcome to BoardBuilder!" translateY="50.0" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
   </top>
   <center>
      <BorderPane maxWidth="660.0" prefHeight="576.0" prefWidth="660.0" BorderPane.alignment="CENTER">
         <bottom>
            <AnchorPane prefHeight="159.0" prefWidth="658.0" BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="343.0" layoutY="29.0" mnemonicParsing="false" onAction="#showAboutScreen" prefHeight="34.0" prefWidth="100.0" text="About Us" />
                  <Button layoutX="196.0" layoutY="29.0" mnemonicParsing="false" onAction="#showGettingStartedScreen" prefHeight="34.0" prefWidth="100.0" text="Getting Started" />
               </children>
            </AnchorPane>
         </bottom>
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="14.0" layoutY="165.0" mnemonicParsing="false" onAction="#switchToSetSizeScreen" prefHeight="80.0" prefWidth="150.0" text="New Project">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button layoutX="251.0" layoutY="315.0" mnemonicParsing="false" onAction="#switchToEditorScreenCurrent" prefHeight="40.0" prefWidth="140.0" text="Current Project">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button layoutX="241.0" layoutY="165.0" mnemonicParsing="false" onAction="#loadSavedFile" prefHeight="80.0" prefWidth="160.0" text="Load  Project">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button layoutX="484.0" layoutY="165.0" mnemonicParsing="false" onAction="#switchToPredesign" prefHeight="80.0" prefWidth="160.0" text="Predesigned ">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
